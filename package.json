{
  "name": "Fontfully",
  "version": "1.0.0",
  "private": true,
  "description": "Change the font of any website",
  "scripts": {
    "clean": "rm -rf ./build",
    "deploy": "zip -r ../fontfully.zip . -x node_modules/\\* -x webpack/\\* -x src/\\* -x .\\* -x config.example.js",
    "eslint": "eslint ./src",
    "server-dev": "webpack --watch --progress --colors --display-error-details -d --config ./webpack/webpack.config.js",
    "validate-webpack": "webpack-validator ./webpack/webpack.config.js"
  },
  "author": "Johnny Ji",
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "node-sass": "^3.8.0",
    "pure-render-decorator": "^1.1.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-immutable-proptypes": "^1.7.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-johnnyji": "^0.1.6",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  }
}
